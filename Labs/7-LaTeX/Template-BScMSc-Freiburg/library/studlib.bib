@Article{calder:1977:mti,
  Author =		 "I. R. Calder",
  Title =		 "A model of transpiration and interception loss from
                  a spruce forest in {P}lynlimon, {C}entral {W}ales",
  Journal =		 jh,
  Volume =		 "33",
  Pages =		 "247--265",
  abstract =	 "A model for evaporation loss (interception plus
                  transpiration) from a spruce forest, based on the
                  Penman-Monteith equation, is derived from lysimeter
                  and automatic weather station data. Values for the
                  aerodynamic resistance ($r_a = 3.5 s m^{-1}$) and
                  the other canopy parameters required by a Rutter
                  type of interception model were obtained using
                  optimisation techniques. These techniques were also
                  used to obtain the parameter values in a
                  tranpiration model which makes use of the emperical
                  surface resistance equation: \begin{equation} r_{sD}
                  = 75.5 \left[ 1- 0.30 cos \left\{ 2 \pi \left(
                  \frac{D - 222}{365} \right) \right\} \right]/
                  (1-0.45\delta e) , \delta e < 2.2 \end{equation}
                  \begin{equation} r_{sD} = \infty, \delta 3 \geq 2.2
                  \end{equation} where $r_{sD}$ is the surface
                  resistance on day No. D and $\delta e$ is the vapour
                  pressure deficit (kPa). Good agreement with
                  observation was found for transpiration predictions
                  at all times. Good predictions of interception loss
                  could be obtained only of (small) zero offset errors
                  in the measurements of vapour pressure deficit were
                  absent. The feasibility of using this model,
                  together with derived parameter values, to predict
                  losses from spruce forest in other locations is
                  discussed.",
  codenumber =	 "173",
  from =		 "bib",
  keywords =	 "interception, daily",
  usedfor =		 "puerto rico",
  year =		 1977
}

