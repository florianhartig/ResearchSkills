@Article{ferguson:1994:mus,
  Author =		 "R. I. Ferguson and S. T. Trudgill and J. Ball",
  Title =		 "Mixing and uptake of solutes in catchments: model
                  development",
  Journal =		 jh,
  Volume =		 "159",
  Pages =		 "223--233",
  abstract =	 "Simple mixing models are widely used to explain
                  temporal variations in streamwater chemistry and to
                  infer the relative contributions of old and new
                  water during runoff events. These models assume
                  conservative mixing of fixed and members and ignore
                  solute uptake while new water is resident in the
                  ground. A simple modeling approach is proposed which
                  combines two-component mixing with solute uptake
                  using a firts order kinetic rate equation. In this
                  approach new and old water have the same origin in
                  precipitation but old water, by virtue of its longer
                  residence time, is more enriched. Equations are
                  presented and illistrated for predicting the
                  chemical evolution of old, new and mixed water and
                  for the inferring new water contributions. One
                  consequence of the model is that solute rating plots
                  do not exibit a unique and reversible
                  concentration-discharge relationship but a set of
                  shifted sounter-clockwise loops. Methods of
                  parameter estimation are discussed. If intensive
                  time-sereis data are not available, estimation of
                  the chemical uptake parameter is not possible for a
                  single solute but is possible for two solutes given
                  information on several runoff events.",
  codenumber =	 "301",
  keywords =	 "mixing modles, stormflow, streamflow chemistry",
  location =	 l_div,
  usedfor =		 "puerto rico",
  year =		 1994
}

